{
    "name": "nodejs-skeleton",
    "version": "1.0.0",
    "description": "NodeJS Skeleton",
    "main": "app.ts",
    "scripts": {
        "start": "tsc && node dist/app.js",
        "dev": "nodemon src/app.ts",
        "build": "tsc -p .",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "prettier-format": "prettier --write 'src/**/*.{ts,js,css,html}'",
        "prettier-check": "prettier --check 'src/**/*.{ts,js,css,html}'",
        "test": "jest -i --passWithNoTests"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.10.1",
        "inversify": "^5.0.5",
        "inversify-binding-decorators": "^4.0.0",
        "inversify-express-utils": "^6.3.2",
        "mongodb": "^4.5.0",
        "nodemon": "^2.0.7",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.9",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.23",
        "@types/mongodb": "^3.6.8",
        "@types/node": "^15.0.1",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@types/uuid": "^8.3.4",
        "eslint": "^7.25.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}
